{
    "name": "symfony/framework-standard-edition",
    "license": "MIT",
    "type": "project",
    "description": "The \"Symfony Standard Edition\" distribution",
    "autoload": {
        "psr-0": { "": "src/", "SymfonyStandard": "app/" }
    },
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "2.5.*",
        "doctrine/orm": "~2.2,>=2.2.3",
        "doctrine/doctrine-bundle": "~1.2",
        "twig/extensions": "~1.0",
        "symfony/assetic-bundle": "~2.3",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~3.0",
        "sensio/framework-extra-bundle": "~3.0",
        "incenteev/composer-parameter-handler": "~2.0",

        "symfony/proxy-manager-bridge": "2.5.*",

        "netgusto/autoroute-bundle": "1.0.0",
        "netgusto/devserver-bundle": "dev-master",

        "friendsofsymfony/rest-bundle": "1.4.0",
        "jms/serializer-bundle": "0.13.*",

        "jms/security-extra-bundle": "1.5.*",

        "leafo/lessphp": "~0.4.0",
        "cebe/markdown": "0.9.2",

        "zipball/highlightjs": "~8.0",
        "requirejs/requirejs": "2.1.11",
        "backbonejs/backbonejs": "1.1.2",
        "modernizr/modernizr": "2.7.1",
        "jquery/jquery": "2.1.0",

        "font-awesome/font-awesome": "~4.1",
        "suin/php-rss-writer": ">=1.0",

        "netgusto/pulpy-theme-dropplets": "~1.0",
        "netgusto/pulpy-theme-medium": "~1.0",
        "netgusto/pulpy-theme-casper": "~1.0",
        "aws/aws-sdk-php": "2.6.7",
        
        "brianium/habitat": "v1.0.0",

        "twbs/bootstrap": "v3.2.0",

        "zipball/purecss": "~0.5",
        "zipball/purecss-layout-sidemenu": "~0.5",
        "zipball/purecss-layout-email": "~0.5",

        "zipball/angularjs": "1.3.0-beta.14",

        "ember/ember": "1.6.0",
        "ember/ember-debug": "1.6.0",
        "ember/ember-data": "1.0.0-beta.8",
        "handlebars/handlebars": "1.3.0"
    },
    "require-dev": {
        "sensio/generator-bundle": "~2.3"
    },
    "repositories": [
        {
            "url": "https://github.com/netgusto/Pulpy-Theme-Dropplets.git",
            "type": "git"
        },
        {
            "url": "https://github.com/netgusto/Pulpy-Theme-Medium.git",
            "type": "git"
        },
        {
            "url": "https://github.com/netgusto/Pulpy-Theme-Casper.git",
            "type": "git"
        },
        {
            "type": "package",
            "package": {
              "name": "requirejs/requirejs",
                "version": "2.1.11",
                "dist": {
                    "url": "http://requirejs.org/docs/release/2.1.11/minified/require.js",
                    "type": "file"
                }
            }
        },
        {
            "type": "package",
            "package": {
              "name": "backbonejs/backbonejs",
                "version": "1.1.2",
                "dist": {
                    "url": "http://backbonejs.org/backbone.js",
                    "type": "file"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "jquery/jquery",
                "version": "2.1.0",
                "dist": {
                    "url": "http://code.jquery.com/jquery-2.1.0.min.js",
                    "type": "file"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "modernizr/modernizr",
                "version": "2.7.1",
                "dist": {
                    "url": "http://cdnjs.cloudflare.com/ajax/libs/modernizr/2.7.1/modernizr.min.js",
                    "type": "file"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "zipball/highlightjs",
                "version": "8.0",
                "dist": {
                    "url": "https://github.com/components/highlightjs/archive/8.0.0.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "font-awesome/font-awesome",
                "version": "4.1.0",
                "dist": {
                    "url": "http://fortawesome.github.io/Font-Awesome/assets/font-awesome-4.1.0.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "zipball/purecss",
                "version": "0.5.0",
                "dist": {
                    "url": "https://github.com/yui/pure-release/archive/v0.5.0.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "zipball/purecss-layout-sidemenu",
                "version": "0.5.0",
                "dist": {
                    "url": "http://purecss.io/layouts/side-menu/download",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "zipball/purecss-layout-email",
                "version": "0.5.0",
                "dist": {
                    "url": "http://purecss.io/layouts/email/download",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "zipball/angularjs",
                "version": "1.3.0-beta.14",
                "dist": {
                    "url": "http://code.angularjs.org/1.3.0-beta.14/angular-1.3.0-beta.14.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "zipball/breezejs",
                "version": "1.4.14",
                "dist": {
                    "url": "http://www.breezejs.com/sites/default/files/packages/breeze-runtime-1.4.14.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "zipball/breezejs-angular",
                "version": "dev-master",
                "dist": {
                    "url": "https://github.com/Breeze/breeze.js.labs/archive/master.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ember/ember",
                "version": "1.6.0",
                "dist": {
                    "url": "http://cdnjs.cloudflare.com/ajax/libs/ember.js/1.6.0/ember.min.js",
                    "type": "file"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ember/ember-debug",
                "version": "1.6.0",
                "dist": {
                    "url": "http://cdnjs.cloudflare.com/ajax/libs/ember.js/1.6.0/ember-debug.js",
                    "type": "file"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ember/ember-data",
                "version": "1.0.0-beta.8",
                "dist": {
                    "url": "http://cdnjs.cloudflare.com/ajax/libs/ember-data.js/1.0.0-beta.8/ember-data.min.js",
                    "type": "file"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "handlebars/handlebars",
                "version": "1.3.0",
                "dist": {
                    "url": "http://builds.handlebarsjs.com.s3.amazonaws.com/handlebars-v1.3.0.js",
                    "type": "file"
                }
            }
        }
    ],
    "scripts": {
        "post-root-package-install": [
            "SymfonyStandard\\Composer::hookRootPackageInstall"
        ],
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Pulpy\\CoreBundle\\Composer\\ScriptHandler::forwardAssetsToWebDir"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Pulpy\\CoreBundle\\Composer\\ScriptHandler::forwardAssetsToWebDir"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "2.5-dev"
        },
        "heroku": {
            "document-root": "web",
            "index-document": "index.php",
            "framework": "silex",
            "php-config": [
                "display_errors=on",
                "log_errors=on"
            ]
        },
        "assetsforwarding": {
            "packages": [
                "requirejs/requirejs",
                "jquery/jquery",
                "modernizr/modernizr",
                "zipball/highlightjs",

                "font-awesome/font-awesome",
                "netgusto/pulpy-theme-dropplets",
                "netgusto/pulpy-theme-medium",
                "netgusto/pulpy-theme-casper",
                
                "twbs/bootstrap",

                "zipball/purecss",
                "zipball/purecss-layout-sidemenu",
                "zipball/purecss-layout-email",
                "zipball/angularjs",

                "ember/ember",
                "ember/ember-debug",
                "ember/ember-data",
                "handlebars/handlebars"
            ]
        }
    }
}
